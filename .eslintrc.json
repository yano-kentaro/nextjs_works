{
  "extends": ["eslint:recommended", "next/core-web-vitals", "prettier"],
  "rules": {
    // 個別のルールを変更、無効にする場合はこちらに追加する
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "sort-imports": "off",
    "import/order": ["error", { "alphabetize": { "order": "asc" } }],
    "import/no-named-as-default-member": "off"
  }
}
